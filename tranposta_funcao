#include <stdio.h>
#include <stdlib.h>

int matriz[10][10];
int posta[10][10];

void preencher(){
    for(int i = 0; i < 10; i++)
    {for(int j = 0; j <10; j++){
        matriz[i][j] = j;
    }

    }
}

void imprimir(int* arrays[10][10]){
    for(int i = 0; i < 10; i++)
    {   for(int j = 0; j <10; j++){
            printf(" %i ", (arrays[i][j]));

    }
        printf("\n");
    }
}

void transpostar(int* posta[10][10],int* matriz[10][10]){
    int aux;
    for(int i = 0; i <10; i++){
        for(int j = 0; j <10; j++){
            aux = matriz[i][j];
            posta[j][i] = aux;
        }
    }
}

int main()
{
    preencher();
    imprimir(matriz);
    printf("\n");
    printf("\n");
    transpostar(posta,matriz);
    imprimir(posta);
    return 0;
}
